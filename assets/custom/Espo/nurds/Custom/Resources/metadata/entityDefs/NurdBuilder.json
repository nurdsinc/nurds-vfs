{
    "fields": {
        "name": {
            "type": "varchar",
            "required": true,
            "pattern": "$noBadCharacters"
        },
        "description": {
            "type": "text"
        },
        "createdAt": {
            "type": "datetime",
            "readOnly": true
        },
        "modifiedAt": {
            "type": "datetime",
            "readOnly": true
        },
        "createdBy": {
            "type": "link",
            "readOnly": true,
            "view": "views/fields/user"
        },
        "modifiedBy": {
            "type": "link",
            "readOnly": true,
            "view": "views/fields/user"
        },
        "assignedUser": {
            "type": "link",
            "required": false,
            "view": "views/fields/assigned-user"
        },
        "teams": {
            "type": "linkMultiple",
            "view": "views/fields/teams"
        },
        "nextBillingDate": {
            "notNull": false,
            "type": "date",
            "isCustom": true
        },
        "lastBillingDate": {
            "notNull": false,
            "type": "date",
            "isCustom": true
        },
        "status": {
            "type": "enum",
            "options": [
                "ACTIVE",
                "EXPIRED",
                "TRIAL",
                "PENDING"
            ],
            "style": {
                "ACTIVE": "success",
                "EXPIRED": "danger",
                "TRIAL": "warning",
                "PENDING": "info"
            },
            "default": "PENDING",
            "displayAsLabel": true,
            "maxLength": 100,
            "isCustom": true
        },
        "overchargeEmails": {
            "notNull": true,
            "type": "bool",
            "isCustom": true
        },
        "billingPeriod": {
            "type": "enum",
            "options": [
                "MONTHLY",
                "YEARLY"
            ],
            "default": "MONTHLY",
            "style": {
                "MONTHLY": null,
                "YEARLY": null
            },
            "maxLength": 100,
            "isCustom": true
        },
        "created": {
            "notNull": false,
            "type": "date",
            "isCustom": true
        },
        "recurringManualSubscription": {
            "notNull": true,
            "type": "bool",
            "isCustom": true
        },
        "inFreeMonth": {
            "notNull": true,
            "type": "bool",
            "isCustom": true
        },
        "projectId": {
            "type": "varchar",
            "maxLength": 100,
            "options": [],
            "isCustom": true
        },
        "plan": {
            "type": "enum",
            "options": [
                "Trial",
                "Personal",
                "Business",
                "Growth",
                "WL Growth"
            ],
            "default": "Trial",
            "style": {
                "Trial": null,
                "Personal": null,
                "Business": null,
                "Growth": null,
                "WL Growth": null
            },
            "maxLength": 100,
            "isCustom": true
        },
        "bandwidthPeriod": {
            "notNull": false,
            "type": "date",
            "isCustom": true
        }
    },
    "links": {
        "createdBy": {
            "type": "belongsTo",
            "entity": "User"
        },
        "modifiedBy": {
            "type": "belongsTo",
            "entity": "User"
        },
        "assignedUser": {
            "type": "belongsTo",
            "entity": "User"
        },
        "teams": {
            "type": "hasMany",
            "entity": "Team",
            "relationName": "entityTeam",
            "layoutRelationshipsDisabled": true
        },
        "meetings": {
            "type": "hasMany",
            "entity": "Meeting",
            "foreign": "parent"
        },
        "calls": {
            "type": "hasMany",
            "entity": "Call",
            "foreign": "parent"
        },
        "tasks": {
            "type": "hasChildren",
            "entity": "Task",
            "foreign": "parent"
        },
        "emails": {
            "type": "hasChildren",
            "entity": "Email",
            "foreign": "parent",
            "layoutRelationshipsDisabled": true
        }
    },
    "collection": {
        "orderBy": "createdAt",
        "order": "desc",
        "textFilterFields": [
            "name"
        ],
        "fullTextSearch": false,
        "countDisabled": false
    },
    "indexes": {
        "name": {
            "columns": [
                "name",
                "deleted"
            ]
        },
        "assignedUser": {
            "columns": [
                "assignedUserId",
                "deleted"
            ]
        },
        "createdAt": {
            "columns": [
                "createdAt"
            ]
        },
        "createdAtId": {
            "unique": true,
            "columns": [
                "createdAt",
                "id"
            ]
        }
    },
    "optimisticConcurrencyControl": false
}