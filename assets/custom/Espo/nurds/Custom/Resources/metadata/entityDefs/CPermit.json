{
    "fields": {
        "name": {
            "type": "varchar",
            "required": true,
            "pattern": "$noBadCharacters"
        },
        "description": {
            "type": "text"
        },
        "createdAt": {
            "type": "datetime",
            "readOnly": true
        },
        "modifiedAt": {
            "type": "datetime",
            "readOnly": true
        },
        "createdBy": {
            "type": "link",
            "readOnly": true,
            "view": "views/fields/user"
        },
        "modifiedBy": {
            "type": "link",
            "readOnly": true,
            "view": "views/fields/user"
        },
        "assignedUser": {
            "type": "link",
            "required": false,
            "view": "views/fields/assigned-user"
        },
        "teams": {
            "type": "linkMultiple",
            "view": "views/fields/teams"
        },
        "permitNumber": {
            "type": "varchar",
            "maxLength": 100,
            "options": [],
            "isCustom": true
        },
        "permitId": {
            "type": "varchar",
            "maxLength": 100,
            "options": [],
            "isCustom": true
        },
        "permitType": {
            "type": "enum",
            "options": [
                "Residential New Construction",
                ""
            ],
            "style": {
                "Residential New Construction": null,
                "": null
            },
            "maxLength": 100,
            "isCustom": true
        },
        "permitStatus": {
            "type": "enum",
            "options": [
                "Complete",
                "Approved",
                "Denied",
                "Expired",
                "Fees Due",
                "Fees Paid",
                "In Review",
                "Issued",
                "On Hold",
                "Plan Approval Expired",
                "Requires Customer Attention",
                "Stop Work Order",
                "Submitted",
                "Submitted - Online",
                "Withdrwan",
                ""
            ],
            "style": {
                "Complete": null,
                "Approved": null,
                "Denied": null,
                "Expired": null,
                "Fees Due": null,
                "Fees Paid": null,
                "In Review": null,
                "Issued": null,
                "On Hold": null,
                "Plan Approval Expired": null,
                "Requires Customer Attention": null,
                "Stop Work Order": null,
                "Submitted": null,
                "Submitted - Online": null,
                "Withdrwan": null,
                "": null
            },
            "maxLength": 100,
            "isCustom": true
        },
        "projectName": {
            "type": "varchar",
            "maxLength": 100,
            "options": [],
            "isCustom": true
        },
        "mainParcel": {
            "type": "varchar",
            "maxLength": 100,
            "options": [],
            "isCustom": true
        },
        "address": {
            "type": "address",
            "isCustom": true
        },
        "appliedDate": {
            "notNull": false,
            "type": "date",
            "isCustom": true
        },
        "issuedDate": {
            "notNull": false,
            "type": "date",
            "isCustom": true
        },
        "finalizedDate": {
            "notNull": false,
            "type": "date",
            "isCustom": true
        },
        "expirationDate": {
            "notNull": false,
            "type": "date",
            "isCustom": true
        },
        "district": {
            "type": "enum",
            "options": [
                "Maricopa County",
                ""
            ],
            "style": {
                "Maricopa County": null,
                "": null
            },
            "maxLength": 100,
            "isCustom": true
        },
        "squareFeet": {
            "notNull": false,
            "type": "float",
            "isCustom": true
        },
        "valuation": {
            "notNull": false,
            "type": "float",
            "isCustom": true
        },
        "json": {
            "type": "text",
            "rowsMin": 2,
            "cutHeight": 200,
            "isCustom": true
        }
    },
    "links": {
        "createdBy": {
            "type": "belongsTo",
            "entity": "User"
        },
        "modifiedBy": {
            "type": "belongsTo",
            "entity": "User"
        },
        "assignedUser": {
            "type": "belongsTo",
            "entity": "User"
        },
        "teams": {
            "type": "hasMany",
            "entity": "Team",
            "relationName": "entityTeam",
            "layoutRelationshipsDisabled": true
        },
        "meetings": {
            "type": "hasMany",
            "entity": "Meeting",
            "foreign": "parent"
        },
        "calls": {
            "type": "hasMany",
            "entity": "Call",
            "foreign": "parent"
        },
        "tasks": {
            "type": "hasChildren",
            "entity": "Task",
            "foreign": "parent"
        },
        "emails": {
            "type": "hasChildren",
            "entity": "Email",
            "foreign": "parent",
            "layoutRelationshipsDisabled": true
        }
    },
    "collection": {
        "orderBy": "createdAt",
        "order": "desc"
    },
    "indexes": {
        "name": {
            "columns": [
                "name",
                "deleted"
            ]
        },
        "assignedUser": {
            "columns": [
                "assignedUserId",
                "deleted"
            ]
        },
        "createdAt": {
            "columns": [
                "createdAt"
            ]
        },
        "createdAtId": {
            "unique": true,
            "columns": [
                "createdAt",
                "id"
            ]
        }
    }
}