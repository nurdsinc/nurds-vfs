{
    "fields": {
        "name": {
            "type": "varchar",
            "required": true,
            "pattern": "$noBadCharacters"
        },
        "description": {
            "type": "text"
        },
        "createdAt": {
            "type": "datetime",
            "readOnly": true
        },
        "modifiedAt": {
            "type": "datetime",
            "readOnly": true
        },
        "createdBy": {
            "type": "link",
            "readOnly": true,
            "view": "views/fields/user"
        },
        "modifiedBy": {
            "type": "link",
            "readOnly": true,
            "view": "views/fields/user"
        },
        "assignedUser": {
            "type": "link",
            "required": false,
            "view": "views/fields/assigned-user"
        },
        "teams": {
            "type": "linkMultiple",
            "view": "views/fields/teams"
        },
        "pickupLocation": {
            "type": "address",
            "isCustom": true
        },
        "dropoffLocation": {
            "type": "address",
            "isCustom": true
        },
        "pickupDate": {
            "notNull": false,
            "type": "datetime",
            "minuteStep": 30,
            "isCustom": true
        },
        "dropoffDate": {
            "notNull": false,
            "type": "datetime",
            "minuteStep": 30,
            "isCustom": true
        },
        "bookedDate": {
            "notNull": false,
            "type": "datetime",
            "minuteStep": 30,
            "isCustom": true
        },
        "vehicle": {
            "type": "link"
        },
        "primaryDriver": {
            "type": "link"
        },
        "status": {
            "type": "enum",
            "options": [
                "Pending",
                "In Process",
                "Booked",
                "Delivered",
                "Picked Up",
                "Trip In Progress",
                "Waiting on Return",
                "Returned",
                "Inspected",
                "Cleaned",
                "Completed"
            ],
            "style": {
                "Pending": "primary",
                "In Process": "primary",
                "Booked": "info",
                "Delivered": "success",
                "Picked Up": "success",
                "Trip In Progress": "warning",
                "Waiting on Return": "warning",
                "Returned": "primary",
                "Inspected": "info",
                "Cleaned": "success",
                "Completed": "success"
            },
            "default": "Pending",
            "displayAsLabel": true,
            "audited": true,
            "maxLength": 100,
            "isCustom": true
        },
        "addOns": {
            "type": "multiEnum",
            "storeArrayValues": true,
            "default": [],
            "options": [
                "Car Seat"
            ],
            "style": {
                "Car Seat": null
            },
            "isCustom": true
        },
        "tripNumber": {
            "type": "number",
            "len": 36,
            "notNull": false,
            "unique": false,
            "nextNumber": 1,
            "padLength": 5,
            "copyToClipboard": true,
            "prefix": "T",
            "isCustom": true
        },
        "creditCard": {
            "type": "link"
        },
        "tags": {
            "type": "multiEnum",
            "storeArrayValues": true,
            "default": [],
            "options": [
                "Emailed",
                "Contacted",
                "Emailed 2Hr Pre-Trip"
            ],
            "style": {
                "Emailed": null,
                "Contacted": null,
                "Emailed 2Hr Pre-Trip": null
            },
            "allowCustomOptions": true,
            "displayAsLabel": true,
            "audited": true,
            "isCustom": true
        },
        "lineItems": {
            "type": "text",
            "rowsMin": 2,
            "cutHeight": 200,
            "isCustom": true
        }
    },
    "links": {
        "createdBy": {
            "type": "belongsTo",
            "entity": "User"
        },
        "modifiedBy": {
            "type": "belongsTo",
            "entity": "User"
        },
        "assignedUser": {
            "type": "belongsTo",
            "entity": "User"
        },
        "teams": {
            "type": "hasMany",
            "entity": "Team",
            "relationName": "entityTeam",
            "layoutRelationshipsDisabled": true
        },
        "meetings": {
            "type": "hasMany",
            "entity": "Meeting",
            "foreign": "parent",
            "layoutRelationshipsDisabled": true
        },
        "calls": {
            "type": "hasMany",
            "entity": "Call",
            "foreign": "parent",
            "layoutRelationshipsDisabled": true
        },
        "tasks": {
            "type": "hasChildren",
            "entity": "Task",
            "foreign": "parent",
            "layoutRelationshipsDisabled": true
        },
        "emails": {
            "type": "hasChildren",
            "entity": "Email",
            "foreign": "parent",
            "layoutRelationshipsDisabled": true
        },
        "vehicle": {
            "type": "belongsTo",
            "foreign": "trips",
            "entity": "CVehicle",
            "audited": false,
            "isCustom": true
        },
        "primaryDriver": {
            "type": "belongsTo",
            "foreign": "cTrips",
            "entity": "Contact",
            "audited": false,
            "isCustom": true
        },
        "additionalDrivers": {
            "type": "hasMany",
            "relationName": "cTripContact",
            "foreign": "cAdditionalTrips",
            "entity": "Contact",
            "audited": true,
            "isCustom": true
        },
        "creditCard": {
            "type": "belongsTo",
            "foreign": "trips",
            "entity": "CCreditCard",
            "audited": false,
            "isCustom": true
        }
    },
    "collection": {
        "orderBy": "createdAt",
        "order": "desc",
        "textFilterFields": [
            "name"
        ],
        "fullTextSearch": false,
        "countDisabled": false
    },
    "indexes": {
        "name": {
            "columns": [
                "name",
                "deleted"
            ]
        },
        "assignedUser": {
            "columns": [
                "assignedUserId",
                "deleted"
            ]
        },
        "createdAt": {
            "columns": [
                "createdAt"
            ]
        },
        "createdAtId": {
            "unique": true,
            "columns": [
                "createdAt",
                "id"
            ]
        }
    },
    "optimisticConcurrencyControl": false
}