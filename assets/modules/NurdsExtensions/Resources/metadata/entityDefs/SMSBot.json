{
    "fields": {
        "name": {
            "type": "varchar",
            "required": true,
            "pattern": "$noBadCharacters"
        },
        "description": {
            "type": "text"
        },
        "createdAt": {
            "type": "datetime",
            "readOnly": true
        },
        "modifiedAt": {
            "type": "datetime",
            "readOnly": true
        },
        "createdBy": {
            "type": "link",
            "readOnly": true,
            "view": "views/fields/user"
        },
        "modifiedBy": {
            "type": "link",
            "readOnly": true,
            "view": "views/fields/user"
        },
        "assignedUser": {
            "type": "link",
            "required": false,
            "view": "views/fields/assigned-user"
        },
        "teams": {
            "type": "linkMultiple",
            "view": "views/fields/teams",
            "autocompleteOnEmpty": true
        },
        "source": {
            "type": "enum",
            "options": [
                "Voxtel",
                "Twiio"
            ],
            "style": {
                "Voxtel": null,
                "Twiio": null
            },
            "default": "Voxtel",
            "maxLength": 100,
            "isCustom": true
        },
        "direction": {
            "type": "enum",
            "options": [
                "Inbound",
                "Outbound",
                "Unknown"
            ],
            "default": "Unknown",
            "style": {
                "Inbound": null,
                "Outbound": null,
                "Unknown": null
            },
            "maxLength": 100,
            "isCustom": true
        },
        "to": {
            "type": "varchar",
            "maxLength": 20,
            "options": [],
            "isCustom": true
        },
        "from": {
            "type": "varchar",
            "maxLength": 20,
            "options": [
                "+14807257807",
                "+14806133033"
            ],
            "isCustom": true,
            "copyToClipboard": true
        },
        "body": {
            "type": "text",
            "rowsMin": 2,
            "cutHeight": 200,
            "maxLength": 140,
            "isCustom": true
        },
        "lead": {
            "type": "link"
        },
        "status": {
            "type": "enum",
            "options": [
                "Pending",
                "Queued",
                "Sent",
                "Received",
                "Delivered",
                "Failed"
            ],
            "style": {
                "Pending": "primary",
                "Queued": "warning",
                "Sent": "info",
                "Received": "success",
                "Delivered": "success",
                "Failed": "danger"
            },
            "default": "Pending",
            "maxLength": 100,
            "isCustom": true,
            "displayAsLabel": true
        },
        "messageId": {
            "type": "varchar",
            "maxLength": 100,
            "options": [],
            "isCustom": true,
            "copyToClipboard": true
        },
        "campaign": {
            "type": "link"
        },
        "contact": {
            "type": "link"
        }
    },
    "links": {
        "createdBy": {
            "type": "belongsTo",
            "entity": "User"
        },
        "modifiedBy": {
            "type": "belongsTo",
            "entity": "User"
        },
        "assignedUser": {
            "type": "belongsTo",
            "entity": "User"
        },
        "teams": {
            "type": "hasMany",
            "entity": "Team",
            "relationName": "entityTeam",
            "layoutRelationshipsDisabled": true
        },
        "lead": {
            "type": "belongsTo",
            "foreign": "SMSBots",
            "entity": "Lead",
            "audited": false,
            "isCustom": true
        },
        "campaign": {
            "type": "belongsTo",
            "foreign": "SMSBots",
            "entity": "Campaign",
            "audited": false,
            "isCustom": true
        },
        "contact": {
            "type": "belongsTo",
            "foreign": "SMSBots",
            "entity": "Contact",
            "audited": true,
            "isCustom": true
        }
    },
    "collection": {
        "orderBy": "createdAt",
        "order": "desc",
        "textFilterFields": [
            "name"
        ],
        "fullTextSearch": false,
        "countDisabled": false
    },
    "indexes": {
        "name": {
            "columns": [
                "name",
                "deleted"
            ]
        },
        "assignedUser": {
            "columns": [
                "assignedUserId",
                "deleted"
            ]
        },
        "createdAt": {
            "columns": [
                "createdAt"
            ]
        },
        "createdAtId": {
            "unique": true,
            "columns": [
                "createdAt",
                "id"
            ]
        }
    },
    "optimisticConcurrencyControl": false
}